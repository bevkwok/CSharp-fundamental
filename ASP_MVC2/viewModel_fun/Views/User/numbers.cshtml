<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Numbers</title>
</head>
<body>
    <div>
        <h1>Here are some numbers!</h1>
    </div>
    <div>
        @model int[]
        @foreach(var number in Model){
            <p>
                @number
            </p>
        }
    </div>
</body>
</html>