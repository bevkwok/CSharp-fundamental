@using viewModel_fun.Models
@model List<User>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>
<body>
    <div>
        <h1>Here are some Users!</h1>
    </div>
    <div>

        @foreach (var user in Model)
        {
            <p>@user.Firstname  @user.Lastname</p>
        }
        
    </div>
</body>
</html>